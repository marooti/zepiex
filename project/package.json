{
  "name": "zappix",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4800",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:zappix-customer:prod": "ng build zappix-customer --configuration=production",
    "build:zappix-customer:qa": "ng build zappix-customer --configuration=qa",
    "build:components": "npm run clean:components && npm run cp-components-assets && ng build @zappix/components",
    "watch:components": "npm run clean:components && npm run cp-components-assets && ng build @zappix/components --watch",
    "cp-components-assets": "cpx \"./projects/zappix/components/src/assets/**/*\" \"./dist/zappix/components/assets\"",
    "clean:components": "rimraf \"./dist/zappix/components*\"",
    "remove:build": "rimraf \"./dist*\"",
    "remove:node_modules": "rimraf \"./node_modules*\"",
    "build:services": "ng build @zappix/services",
    "watch:services": "ng build @zappix/services --watch",
    "build:domain": "ng build @zappix/domain",
    "watch:domain": "ng build @zappix/domain --watch",
    "build:ngx-intl-tel-input": "ng build @zappix/ngx-intl-tel-input",
    "watch:ngx-intl-tel-input": "ng build @zappix/ngx-intl-tel-input --watch",
    "uninstall:dependencies": "npm uninstall @zappix/ngx-intl-tel-input --save && npm uninstall @zappix/components --save && npm uninstall @zappix/services && npm uninstall @zappix/domain --save",
    "setup:project": "npm ci",
    "setup:npm-packages": "npm ci && npm run install:dependencies",
    "build:local-packages": "npm run build:ngx-intl-tel-input && npm run build:components && npm run build:services && npm run build:domain",
    "install:local-packages": "npm i dist/zappix/ngx-intl-tel-input --save && npm i dist/zappix/components --save && npm i dist/zappix/services --save && npm i dist/zappix/domain --save",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^14.1.0",
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^14.2.0",
    "@angular/compiler": "^14.2.0",
    "@angular/core": "^14.2.0",
    "@angular/forms": "^14.2.0",
    "@angular/google-maps": "^14.2.7",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.2.0",
    "@angular/platform-browser-dynamic": "^14.2.0",
    "@angular/router": "^14.2.0",
    "@e-signature/ng-signature-pad": "^0.0.6",
    "@fullcalendar/daygrid": "^6.1.8",
    "@zapix/components": "file:dist/zappix/components",
    "@zapix/domain": "file:dist/zappix/domain",
    "@zapix/ngx-intl-tel-input": "file:dist/zappix/ngx-intl-tel-input",
    "@zapix/services": "file:dist/zappix/services",
    "@zappix/ngx-intl-tel-input": "file:dist/zappix/ngx-intl-tel-input",
    "amazon-cognito-identity-js": "^5.2.12",
    "angularx-qrcode": "^14.0.0",
    "aws-sdk": "^2.1313.0",
    "chart.js": "^4.3.0",
    "compressorjs": "^1.2.1",
    "cpx": "^1.5.0",
    "date-fns": "^2.30.0",
    "exceljs": "^4.3.0",
    "faker": "^6.6.6",
    "file-saver": "^2.0.5",
    "google-libphonenumber": "^3.2.32",
    "html2canvas": "^1.4.1",
    "image-compression": "^0.0.6",
    "image-size": "^1.0.2",
    "intl-tel-input": "^17.0.21",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.6.0",
    "libphonenumber-js": "^1.10.26",
    "lodash": "^4.17.21",
    "lodash.range": "^3.2.0",
    "luxon": "^3.4.4",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "ng2-pdf-viewer": "^7.0.0",
    "ngx-bootstrap": "^10.0.0",
    "ngx-extended-pdf-viewer": "^18.1.14",
    "ngx-image-compress": "^15.1.6",
    "ngx-material-timepicker": "^13.1.1",
    "null-loader": "^4.0.1",
    "papaparse": "^5.4.0",
    "pdf-lib": "^1.17.1",
    "pdf2pic": "^3.1.1",
    "pdfjs-dist": "^3.2.146",
    "primeflex": "^3.3.0",
    "primeicons": "^6.0.1",
    "primeng": "^14.2.3",
    "quill": "^1.3.7",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "webpack": "^5.76.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.9",
    "@angular-eslint/builder": "14.2.0",
    "@angular-eslint/eslint-plugin": "14.2.0",
    "@angular-eslint/eslint-plugin-template": "14.2.0",
    "@angular-eslint/schematics": "14.2.0",
    "@angular-eslint/template-parser": "14.2.0",
    "@angular/cli": "~14.2.4",
    "@angular/compiler-cli": "^14.2.0",
    "@types/jasmine": "~4.0.0",
    "@types/pdfjs-dist": "^2.10.378",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "jasmine-core": "~4.3.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-packagr": "^14.2.0",
    "ngx-toastr": "^15.2.2",
    "typescript": "~4.7.2"
  }
}
